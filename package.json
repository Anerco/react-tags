{
  "name": "react-tag-input",
  "version": "0.0.0-semantically-released",
  "description": "React tags is a fantastically simple tagging component for your React projects",
  "main": "dist-modules/components/ReactTags.js",
  "files": [
    "dist-modules",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "jest --notify --coverage",
    "test:watch": "jest --watch --notify --coverage",
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "format": "prettier  --write src/*.js __tests__/*.js",
    "build": "webpack --config webpack-production.config.js --progress --colors && babel src --out-dir dist-modules --copy-files",
    "precommit": "./set_up_hooks.sh",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "drag-drop",
    "tags",
    "tag input",
    "react-component",
    "autosuggest"
  ],
  "author": "Prakhar Srivastav",
  "license": "MIT",
  "repository": "https://github.com/prakhar1989/react-tags",
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^3.0.2",
    "react-dom": "^16.3.1"
  },
  "dependencies": {
    "classnames": "~2.2.6",
    "core-js": "^3.6.4",
    "lodash": "~4.17.12",
    "memoize-one": "^5.1.1",
    "prop-types": "~15.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "commitizen": "^4.0.3",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "^3.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^6.4.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.3",
    "greenkeeper-lockfile": "^1.15.1",
    "jest": "^25.1.0",
    "jsdom": "^16.2.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.4",
    "opn": "^6.0.0",
    "prettier": "^1.17.0",
    "raf": "^3.4.1",
    "react": "^16.4.2",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^3.0.2",
    "react-dnd-test-backend": "^3.0.2",
    "react-dom": "^16.4.2",
    "react-test-renderer": "^16.4.2",
    "sass-loader": "^8.0.2",
    "semantic-release": "^17.0.4",
    "sinon": "^9.0.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.14.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.14"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
